@model MarioAndresJaramillo.Models.Turno

@{
    ViewData["Title"] = "Eliminar";
}

<h2>Eliminar turno</h2>

<p>¿Estás seguro de que deseas eliminar el turno con ID @Model.Id?</p>

<form asp-action="Eliminar" method="post">
    <input type="hidden" asp-for="Id" />

    <button type="submit" class="btn btn-danger">Eliminar</button>
</form>

<a href="@Url.Action(nameof(Index))">Volver</a>

